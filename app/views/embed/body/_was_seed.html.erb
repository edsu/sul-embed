<div class='sul-embed-body sul-embed-was-seed-container'
      data-sul-embed-theme="<%= viewer.asset_url('was_seed.css') %>"
      data-sul-icons="<%= viewer.asset_url('sul_icons.css') %>">
  <div class='sul-embed-a' data-sul-thumbs-list-count="<%= viewer.capture_list.length %>">
    <ul class='sul-embed-b'>
      <% viewer.capture_list.each do |memento| %>
        <li>
          <%= viewer.format_memento_datetime(memento.datetime) %>
        </li>
      <% end %>
    </ul>
  </div>
  <script>;jQuery.ajax({url: "<%= asset_url('was_seed.js') %>", cache: true, dataType: 'script'});</script>
</div>
